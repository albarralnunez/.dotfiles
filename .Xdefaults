URxvt*scrollBar:         false
URxvt*scrollBar_right:   false

URxvt*transparent:  true
URxvt*shading:      50
URxvt*depth:        32
URxvt*background:   rgba:0000/0000/0200/6464
URxvt.font:         xft:DejaVu Sans Mono Nerd Font:size=14:antialias=true:
URxvt.boldFont:     xft:DejaVu Sans Mono Nerd Font:bold:size=14:antialias=true:
URxvt.letterSpace:  0

*antialias:  true
*autohint:   true

!!! Extensions
URxvt.perl-ext-common:	default,resize-font,url-select,keyboard-select
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-p: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
!! URxvt.url-launcher:	firefox
!! URxvt.matcher.button:	1

